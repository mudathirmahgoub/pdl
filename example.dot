@startuml
digraph foo {
node [style=rounded]
node1 [shape=box]
node2 [fillcolor=yellow, style="rounded,filled", shape=diamond]
node3 [shape=record, label="{ a | b | c }"]

node1 -> node2[label="a", weight="5.0"]
node2 -> node3[weight="100"]
}
@enduml

